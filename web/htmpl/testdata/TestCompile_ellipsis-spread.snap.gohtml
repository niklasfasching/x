    {{with (dict "caller" "test-noop-ellipsis-spread" "slots" (list) "" .)}}
      .={{.}}
    {{end}}
  {{end}}
</noscript>

<noscript name="ellipsis-spread/test-noop-ellipsis-spread/compiled:<y>">
  .={{.}}
</noscript>

